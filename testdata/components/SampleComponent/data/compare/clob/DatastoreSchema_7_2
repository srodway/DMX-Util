    <xs:element name="Application">
        <xs:annotation>
            <xs:documentation>Top level application table</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence minOccurs="0">
                <xs:element ref="Person" minOccurs="0" maxOccurs="unbounded" />
                <xs:element ref="Assessment" minOccurs="0" maxOccurs="unbounded" />
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:element name="Person">
        <xs:annotation>
            <xs:documentation>Holds the answers for each person</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:attribute name="personID" type="d:SVR_KEY">
                <xs:annotation>
                    <xs:documentation>The identifier for the person</xs:documentation>
                </xs:annotation>
            </xs:attribute>
            <xs:attribute name="firstName" type="IEG_STRING">
                <xs:annotation>
                    <xs:documentation>The persons first name</xs:documentation>
                </xs:annotation>
            </xs:attribute>
            <xs:attribute name="gender" type="IEG_GENDER">
                <xs:annotation>
                    <xs:documentation>The persons gender</xs:documentation>
                </xs:annotation>
            </xs:attribute>
            <xs:attribute name="dateOfBirth" type="IEG_DATE">
                <xs:annotation>
                    <xs:documentation>The persons date of birth</xs:documentation>
                </xs:annotation>
            </xs:attribute>
            <xs:attribute name="roleID" type="IEG_INT64">
                <xs:annotation>
                    <xs:documentation>The id of the assessment delivery role record</xs:documentation>
                </xs:annotation>
            </xs:attribute>
            <xs:attribute name="participantID" type="IEG_INT64">
                <xs:annotation>
                    <xs:documentation>Identifier of the person. Used for locating instances of person entities on the data store the relate to a single individual</xs:documentation>
                </xs:annotation>
            </xs:attribute>
            <xs:attribute name="questionsApplicableInd" type="IEG_BOOLEAN">
                <xs:annotation>
                    <xs:documentation>Indicates if the assessment questions should be asked of this role.</xs:documentation>
                </xs:annotation>
            </xs:attribute>
            <xs:attribute name="emotionalBehavioral" type="CSN_EMOTIONAL_BEHAVIORAL">
                <xs:annotation>
                    <xs:documentation>Answer to the Emotional Behavioral assessment question</xs:documentation>
                </xs:annotation>
            </xs:attribute>
            <xs:attribute name="childDevelopment" type="CSN_CHILD_DEVELOPMENT">
                <xs:annotation>
                    <xs:documentation>Answer to the child development assessment question</xs:documentation>
                </xs:annotation>
            </xs:attribute>
            <xs:attribute name="culturalIdentity" type="CSN_CULTURAL_IDENTITY">
                <xs:annotation>
                    <xs:documentation>Answer to the cultural identity assessment question</xs:documentation>
                </xs:annotation>
            </xs:attribute>
            <xs:attribute name="delinquentBehavior" type="CSN_DELINQUENT_BEHAVIOR">
                <xs:annotation>
                    <xs:documentation>Answer to the delinquent behavior assessment question</xs:documentation>
                </xs:annotation>
            </xs:attribute>
            <xs:attribute name="education" type="CSN_EDUCATION">
                <xs:annotation>
                    <xs:documentation>Answer to the education assessment question</xs:documentation>
                </xs:annotation>
            </xs:attribute>
            <xs:attribute name="familyRelationships" type="CSN_FAMILY_RELATIONSHIPS">
                <xs:annotation>
                    <xs:documentation>Answer to the family relationships assessment question</xs:documentation>
                </xs:annotation>
            </xs:attribute>
            <xs:attribute name="medicalPhysical" type="CSN_MEDICAL_PHYSICAL">
                <xs:annotation>
                    <xs:documentation>Answer to the medical physical assessment question</xs:documentation>
                </xs:annotation>
            </xs:attribute>
            <xs:attribute name="socialRelationship" type="CSN_SOCIAL_RELATIONSHIP">
                <xs:annotation>
                    <xs:documentation>Answer to the social relationship assessment question</xs:documentation>
                </xs:annotation>
            </xs:attribute>
            <xs:attribute name="specializedEducation" type="CSN_SPECIALIZED_EDUCATION">
                <xs:annotation>
                    <xs:documentation>Answer to the specialized education assessment question</xs:documentation>
                </xs:annotation>
            </xs:attribute>
            <xs:attribute name="substanceUse" type="CSN_SUBSTANCE_ABUSE">
                <xs:annotation>
                    <xs:documentation>Answer to the substance use assessment question</xs:documentation>
                </xs:annotation>
            </xs:attribute>
            <xs:attribute name="educationPlanDescription" type="IEG_STRING">
                <xs:annotation>
                    <xs:documentation>Description of the education plan</xs:documentation>
                </xs:annotation>
            </xs:attribute>
            <xs:attribute name="identifiedStrengthNeed" type="CSN_IDENTIFIED_NEED">
                <xs:annotation>
                    <xs:documentation>Answer to the identified strength need question</xs:documentation>
                </xs:annotation>
            </xs:attribute>
            <xs:attribute name="comment" type="IEG_STRING">
		<xs:annotation>
		    <xs:documentation>Comment on the identified strength need question</xs:documentation>
		</xs:annotation>
            </xs:attribute>
        </xs:complexType>
        <xs:key name="Person_Key">
            <xs:selector xpath="./Person" />
            <xs:field xpath="@personID" />
        </xs:key>
    </xs:element>
    <xs:element name="Assessment">
        <xs:annotation>
            <xs:documentation>Indicates which factors should be assessed as part of the assessment</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:attribute name="initialAssessmentInd" type="IEG_BOOLEAN">
                <xs:annotation>
                    <xs:documentation>Indicates if this is the first time this assessment is being executed</xs:documentation>
                </xs:annotation>
            </xs:attribute>
        </xs:complexType>
    </xs:element>
