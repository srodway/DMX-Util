            <fo:block  xsl:use-attribute-sets="Normal_13">Responsibility</fo:block>
          </fo:table-cell>
          <fo:table-cell border-bottom-style="solid" border-bottom-width="0.5pt"
            border-top-style="solid" border-top-width="0.5pt" border-end-style="solid"
            border-end-width="0.5pt">
            <fo:block  xsl:use-attribute-sets="Normal_13">Cover Period</fo:block>
          </fo:table-cell>
          <fo:table-cell border-bottom-style="solid" border-bottom-width="0.5pt"
            border-top-style="solid" border-top-width="0.5pt" border-end-style="solid"
            border-end-width="0.5pt">
            <fo:block  xsl:use-attribute-sets="Normal_13">Participation</fo:block>
          </fo:table-cell>
          <fo:table-cell border-bottom-style="solid" border-bottom-width="0.5pt"
            border-top-style="solid" border-top-width="0.5pt" border-end-style="solid"
            border-end-width="0.5pt">
            <fo:block  xsl:use-attribute-sets="Normal_13">Provider</fo:block>
          </fo:table-cell>
        </fo:table-header>
        <fo:table-body>
          <xsl:for-each select="//STRUCT[SNAME='AgreementActivityDetails']">
            <fo:table-row>
              <fo:table-cell padding-left="3pt" padding-right="3pt" border-bottom-style="solid"
                border-bottom-width="0.5pt" border-start-style="solid" border-start-width="0.5pt"
                border-end-style="solid" border-end-width="0.5pt">
                <fo:block  xsl:use-attribute-sets="Normal_14">
                  <xsl:apply-templates select="FIELD[FNAME='activityName']"/>
                </fo:block>
              </fo:table-cell>
