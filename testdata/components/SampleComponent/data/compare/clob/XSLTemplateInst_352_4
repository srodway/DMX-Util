        <fo:table-column column-width="proportional-column-width(20)"/>
        <fo:table-column column-width="proportional-column-width(20)"/>
        <fo:table-column column-width="proportional-column-width(20)"/>
        <fo:table-column column-width="proportional-column-width(20)"/>
        <fo:table-column column-width="proportional-column-width(20)"/>

        <fo:table-header>

          <fo:table-cell border-bottom-style="solid" border-bottom-width="0.5pt"
            border-top-style="solid" border-top-width="0.5pt" border-start-style="solid"
