                border-bottom-width="0.5pt" border-end-style="solid" border-end-width="0.5pt">
                <fo:block  xsl:use-attribute-sets="Normal_14">
                  <xsl:apply-templates select="FIELD[FNAME='activityClients']"/>
                </fo:block>
              </fo:table-cell>
              <fo:table-cell padding-left="3pt" padding-right="3pt " border-bottom-style="solid"
                border-bottom-width="0.5pt" border-end-style="solid" border-end-width="0.5pt">
                <fo:block  xsl:use-attribute-sets="Normal_14">
                  <xsl:apply-templates select="FIELD[FNAME='activityCoverPeriod']"/>
                </fo:block>
              </fo:table-cell>
              <fo:table-cell padding-left="3pt" padding-right="3pt" border-bottom-style="solid"
                border-bottom-width="0.5pt" border-end-style="solid" border-end-width="0.5pt">
                <xsl:variable name="activityParticipation">
                  <xsl:value-of select="FIELD[FNAME='activityParticipation']/VALUE"/>
                </xsl:variable>
                <xsl:if test="string-length($activityParticipation) > 0 ">
                  <fo:block  xsl:use-attribute-sets="Normal_14">
                    <xsl:apply-templates select="FIELD[FNAME='activityParticipation']"/>
                  </fo:block>
                </xsl:if>
                <xsl:if test="string-length($activityParticipation) = 0 ">
                  <fo:block  xsl:use-attribute-sets="Normal_14">N/A</fo:block>
                </xsl:if>
              </fo:table-cell>
              <fo:table-cell padding-left="3pt" padding-right="3pt" border-bottom-style="solid"
                border-bottom-width="0.5pt" border-end-style="solid" border-end-width="0.5pt">
                <xsl:variable name="providerAddress">
                  <xsl:value-of select="FIELD[FNAME='providerAddress']/VALUE"/>
                </xsl:variable>
                <xsl:if test="string-length($providerAddress) > 0 ">
                  <fo:block  xsl:use-attribute-sets="Normal_14">
                    <xsl:apply-templates select="FIELD[FNAME='providerName']"/>,
                      <xsl:apply-templates select="FIELD[FNAME='providerAddress']"/>
                  </fo:block>
                </xsl:if>
                <xsl:if test="string-length($providerAddress) = 0 ">
                  <fo:block  xsl:use-attribute-sets="Normal_14">N/A</fo:block>
                </xsl:if>
              </fo:table-cell>
            </fo:table-row>
          </xsl:for-each>
        </fo:table-body>
      </fo:table>
    </xsl:if>


    <xsl:if test="$numberOfActivities = 0">
      <fo:table table-layout="fixed" width="100%">

        <fo:table-column column-width="proportional-column-width(100)"/>

        <fo:table-body>

          <fo:table-row>
            <fo:table-cell padding-left="3pt" padding-right="3pt" border-bottom-style="solid"
              border-top-style="solid" border-top-width="0.5pt" border-bottom-width="0.5pt"
              border-start-style="solid" border-start-width="0.5pt" border-end-style="solid"
              border-end-width="0.5pt" padding-top="5pt" padding-bottom="5pt">
              <fo:block  xsl:use-attribute-sets="Normal_15"> No Activities Currently Exist For This
                Agreement </fo:block>
            </fo:table-cell>

          </fo:table-row>

        </fo:table-body>
      </fo:table>
    </xsl:if>
  </xsl:template>



  <xsl:template match="FIELD">
    <xsl:value-of select="VALUE"/>
  </xsl:template>

</xsl:stylesheet>
