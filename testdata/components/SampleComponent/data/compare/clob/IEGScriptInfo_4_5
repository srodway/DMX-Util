        <question-page
          entity="Group"
          id="SafetyInterventions"
          progress="0"
          read-only="false"
          set-focus="true"
          show-back-button="true"
          show-exit-button="true"
          show-next-button="true"
          show-person-tabs="false"
          show-save-exit-button="true"
        >
          <title id="QuestionPage.SafetyInterventions.Title">SDM\u00AE Reunification Assessment</title>
          <validation expression="ValidateSafetyInterventions(Group.voluntaryPlacement,Group.protectiveCustody)">
            <message id="SafetyInterventions.validation.message">Both questions 9. and 10. can not
              be answered 'Yes'.</message>
          </validation>
          <validation expression="Group.specifyOther!=&quot;RAI10000&quot; or Group.otherDetails!=&quot;&quot;">
            <message id="Question.Validation.SI8">When question '8. Other' is selected, a
              description of the other safety intervention must be entered.</message>
          </validation>
          <validation expression="ValidateSafetyInterventionsAllNo(Group.directServices,Group.safetyResources,Group.communityAgencies,Group.protectVictim,Group.leaveHome,Group.safeEnvironment,Group.plannedLegalAction,Group.specifyOther,Group.voluntaryPlacement,Group.protectiveCustody)">
            <message id="SafetyInterventionsAllNo.validation.message">When questions '1-8' are answered 'No', question '9 or 10' must be answered 'Yes'.</message>
          </validation>
