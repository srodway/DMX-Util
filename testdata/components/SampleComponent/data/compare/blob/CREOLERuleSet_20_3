  <Class extends="AbstractFactorList" name="FactorList">
    <Attribute name="factors">
      <type>
        <javaclass name="List">
          <ruleclass name="Factor"/>
        </javaclass>
      </type>
      <derivation>
        <fixedlist>
          <listof>
            <ruleclass name="Factor"/>
          </listof>
          <members>
            <reference attribute="determinedSafetyFactor"/>
          </members>
        </fixedlist>
      </derivation>
    </Attribute>
    <Attribute name="determinedSafetyFactor">
      <type>
        <ruleclass name="DeterminedSafety"/>
      </type>
      <derivation>
        <create ruleclass="DeterminedSafety"/>
      </derivation>
    </Attribute>
  </Class>
  <Class extends="Factor" name="DeterminedSafety">
    <!-- Reference of this factor -->
    <Attribute name="reference">
      <type>
        <javaclass name="String"/>
      </type>
      <derivation>
        <String value="SAFETY"/>
      </derivation>
    </Attribute>
    <Attribute name="roleID">
      <type>
        <javaclass name="Long"/>
      </type>
      <derivation>
        <specified/>
      </derivation>
    </Attribute>
    <Attribute name="determinedSafety">
      <type>
        <ruleclass name="DeterminedSafety"/>
      </type>
      <derivation>
        <singleitem onEmpty="error" onMultiple="error">
          <readall ruleclass="DeterminedSafety"/>
        </singleitem>
      </derivation>
    </Attribute>
    <!-- Classification for 'safe'-->
    <Attribute name="classification">
      <type>
        <codetableentry table="ClassDivisionType"/>
      </type>
      <derivation>
        <choose>
          <type>
            <codetableentry table="ClassDivisionType"/>
          </type>
          <when>
            <condition>
              <all>
                <fixedlist>
                  <listof>
                    <javaclass name="Boolean"/>
                  </listof>
                  <members>
                    <equals>
                      <reference attribute="physicalHarm">
                        <reference attribute="reunificationAssessment"/>
                      </reference>
                      <Code table="RUAIndicator">
                        <String value="RAI10001"/>
                      </Code>
                    </equals>
                    <equals>
                      <reference attribute="previousIncidents">
                        <reference attribute="reunificationAssessment"/>
                      </reference>
                      <Code table="RUAIndicator">
                        <String value="RAI10001"/>
                      </Code>
                    </equals>
                    <equals>
                      <reference attribute="sexualAbuse">
                        <reference attribute="reunificationAssessment"/>
                      </reference>
                      <Code table="RUAIndicator">
                        <String value="RAI10001"/>
                      </Code>
                    </equals>
                    <equals>
                      <reference attribute="threatenedHarm">
                        <reference attribute="reunificationAssessment"/>
                      </reference>
                      <Code table="RUAIndicator">
                        <String value="RAI10001"/>
                      </Code>
                    </equals>
                    <equals>
                      <reference attribute="immediateConcern">
                        <reference attribute="reunificationAssessment"/>
                      </reference>
                      <Code table="RUAIndicator">
                        <String value="RAI10001"/>
                      </Code>
                    </equals>
                    <equals>
                      <reference attribute="refusingAccess">
                        <reference attribute="reunificationAssessment"/>
                      </reference>
                      <Code table="RUAIndicator">
                        <String value="RAI10001"/>
                      </Code>
                    </equals>
                    <equals>
                      <reference attribute="immediateNeeds">
                        <reference attribute="reunificationAssessment"/>
                      </reference>
                      <Code table="RUAIndicator">
                        <String value="RAI10001"/>
                      </Code>
                    </equals>
                    <equals>
                      <reference attribute="livingConditions">
                        <reference attribute="reunificationAssessment"/>
                      </reference>
                      <Code table="RUAIndicator">
                        <String value="RAI10001"/>
                      </Code>
                    </equals>
                    <equals>
                      <reference attribute="affectingAbility">
                        <reference attribute="reunificationAssessment"/>
                      </reference>
                      <Code table="RUAIndicator">
                        <String value="RAI10001"/>
                      </Code>
                    </equals>
                    <equals>
                      <reference attribute="domesticViolence">
                        <reference attribute="reunificationAssessment"/>
                      </reference>
                      <Code table="RUAIndicator">
                        <String value="RAI10001"/>
                      </Code>
                    </equals>
                    <equals>
                      <reference attribute="negativeTerms">
                        <reference attribute="reunificationAssessment"/>
                      </reference>
                      <Code table="RUAIndicator">
                        <String value="RAI10001"/>
                      </Code>
                    </equals>
                    <equals>
                      <reference attribute="emotionalStability">
                        <reference attribute="reunificationAssessment"/>
                      </reference>
                      <Code table="RUAIndicator">
                        <String value="RAI10001"/>
                      </Code>
                    </equals>
                    <equals>
                      <reference attribute="other">
                        <reference attribute="reunificationAssessment"/>
                      </reference>
                      <Code table="RUAIndicator">
                        <String value="RAI10001"/>
                      </Code>
                    </equals>
                  </members>
                </fixedlist>
              </all>
            </condition>
            <value>
              <Code table="ClassDivisionType">
                <String value="CDT8258"/>
              </Code>
            </value>
          </when>
          <otherwise>
            <value>
              <reference attribute="unsafeOrConditionallySafe"/>
            </value>
          </otherwise>
        </choose>
      </derivation>
    </Attribute>
