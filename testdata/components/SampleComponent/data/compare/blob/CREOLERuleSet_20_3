  <Class abstract="true" extends="AbstractFactor" name="Factor">
    <Attribute name="assessment">
      <type>
        <ruleclass name="Assessment"/>
      </type>
      <derivation>
        <singleitem onEmpty="error" onMultiple="error">
          <readall ruleclass="Assessment"/>
        </singleitem>
      </derivation>
    </Attribute>
    <Attribute name="reunificationAssessment">
      <type>
        <ruleclass name="Group"/>
      </type>
      <derivation>
        <singleitem onEmpty="error" onMultiple="error">
          <readall ruleclass="Group"/>
        </singleitem>
      </derivation>
    </Attribute>
    <Attribute name="classificationPriority">
      <type>
        <javaclass name="Number"/>
      </type>
      <derivation>
        <choose>
          <type>
            <javaclass name="Number"/>
          </type>
          <test>
            <reference attribute="classification"/>
          </test>
          <when>
            <condition>
              <Code table="ClassDivisionType">
                <String value="CDT8258"/>
              </Code>
            </condition>
            <value>
              <Number value="1"/>
            </value>
          </when>
          <when>
            <condition>
              <Code table="ClassDivisionType">
                <String value="CDT8259"/>
              </Code>
            </condition>
            <value>
              <Number value="2"/>
            </value>
          </when>
          <when>
            <condition>
              <Code table="ClassDivisionType">
                <String value="CDT8260"/>
              </Code>
            </condition>
            <value>
              <Number value="3"/>
            </value>
          </when>
          <otherwise>
            <value>
              <null/>
            </value>
          </otherwise>
        </choose>
      </derivation>
    </Attribute>
    <Attribute name="factorList">
      <type>
        <ruleclass name="FactorList"/>
      </type>
      <derivation>
        <singleitem onEmpty="error" onMultiple="error">
          <readall ruleclass="FactorList"/>
        </singleitem>
      </derivation>
    </Attribute>
  </Class>
  <!-- This class must be extended by all rule sets invoked by the Assessment 
      and planning processing. This class is used to hold the list of factors 
      that should be assessed during the assessment-->
  <Class abstract="true" name="AbstractFactorList">
    <Attribute name="factors">
      <type>
        <javaclass name="List">
          <ruleclass name="AbstractFactor"/>
        </javaclass>
      </type>
      <derivation>
        <!-- Subclasses of AbstractFactorList must override
            this attribute to create a list of the factors
            supported by the rule set. -->
        <abstract/>
      </derivation>
    </Attribute>
  </Class>
  <Class abstract="true" name="AbstractFactor">
    <!-- The reference for the factor. -->
    <Attribute name="reference">
      <type>
        <javaclass name="String"/>
      </type>
      <derivation>
        <abstract/>
      </derivation>
    </Attribute>
    <!-- The id of role that the factor is related to. -->
    <Attribute name="roleID">
      <type>
        <javaclass name="Long"/>
      </type>
      <derivation>
        <abstract/>
      </derivation>
    </Attribute>
    <!-- The score of the factor. Either a score or classification must be provided, 
        if both are provided the classification is used -->
    <Attribute name="score">
      <type>
        <javaclass name="String"/>
      </type>
      <derivation>
        <String value="0"/>
      </derivation>
    </Attribute>
    <!-- The classification of the factor. Either a score or classification must be provided, 
        if both are provided the classification is used -->
    <Attribute name="classification">
      <type>
        <codetableentry table="ClassDivisionType"/>
      </type>
      <derivation>
        <abstract/>
      </derivation>
    </Attribute>
  </Class>
