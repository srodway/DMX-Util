  to the rule classes in the ProductDisplay rule set. --><RuleSet xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" name="DefaultProductKeyDataRuleSet" xsi:noNamespaceSchemaLocation="http://www.curamsoftware.com/CreoleRulesSchema.xsd">


  <!-- Default implementation of AbstractCase. -->
  <Class abstract="true" extends="AbstractCase" extendsRuleSet="ProductKeyDataRuleSet" name="DefaultCase">


    <!-- Simple English-only description. -->
    <Attribute name="description">
      <type>
        <javaclass name="curam.creole.value.Message"/>
      </type>
      <derivation>
        <XmlMessage>Milestone data rules for case: <replace>
            <!-- use toString so that the number is not formatted with commas nor precision truncated -->
            <property name="toString">
              <object>
                <reference attribute="caseID">
                  <reference attribute="productDeliveryCase"/>
                </reference>
              </object>
            </property>
          </replace></XmlMessage>
      </derivation>
    </Attribute>


    <Attribute name="productDeliveryCase">
      <type>
        <ruleclass name="ProductDeliveryCase" ruleset="ProductEligibilityEntitlementRuleSet"/>
      </type>
      <derivation>
        <specified/>
      </derivation>
    </Attribute>


    <!-- No key data - i.e. an empty list -->
    <Attribute name="keyDataTimelines">
      <type>
        <javaclass name="List">
          <ruleclass name="AbstractKeyDataTimeline" ruleset="ProductKeyDataRuleSet"/>
        </javaclass>
      </type>
      <derivation>
        <fixedlist>
          <listof>
            <ruleclass name="AbstractKeyDataTimeline" ruleset="ProductKeyDataRuleSet"/>
          </listof>
          <members/>
        </fixedlist>
      </derivation>
    </Attribute>


  </Class>


