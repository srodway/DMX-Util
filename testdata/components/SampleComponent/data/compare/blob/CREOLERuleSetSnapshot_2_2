            <object>
              <reference attribute="creoleRuleClassLinkID"/>
            </object>
          </property>
        </ResourceMessage>
      </derivation>
    </Attribute>


    <!-- The identifier of this rule class link. -->
    <Attribute name="creoleRuleClassLinkID">
      <type>
        <javaclass name="Number"/>
