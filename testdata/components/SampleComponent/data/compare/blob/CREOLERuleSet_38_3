          <ruleclass name="AbstractAdviceItem" ruleset="CoreAdvisorRuleSet"/>
        </javaclass>
      </type>
      <derivation>
        <joinlists>
          <fixedlist>
            <listof>
              <javaclass name="List">
                <ruleclass name="AbstractAdviceItem" ruleset="CoreAdvisorRuleSet"/>
              </javaclass>
            </listof>
            <members>
              <reference attribute="resolveProspectPersonAdvice"/>
              <reference attribute="resolveProspectsEvidenceAdvice"/>
              <reference attribute="checkEligibilityAdvice"/>


            </members>
          </fixedlist>
        </joinlists>
      </derivation>
    </Attribute>


