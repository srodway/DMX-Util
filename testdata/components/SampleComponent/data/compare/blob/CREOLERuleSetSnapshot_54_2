    <Attribute name="isInAgeRangeToBirthDateTimeline">
      <type>
        <javaclass name="curam.creole.value.Timeline">
          <javaclass name="Boolean"/>
        </javaclass>
      </type>
      <derivation>
        <timelineoperation>
          <all>
            <fixedlist>
              <listof>
                <javaclass name="Boolean"/>
              </listof>
              <members>
                <intervalvalue>
                  <reference attribute="isAliveTimeline"/>
                </intervalvalue>
                <all>
                  <fixedlist>
                    <listof>
                      <javaclass name="Boolean"/>
                    </listof>
                    <members>
                      <compare comparison="&gt;=">
                        <intervalvalue>
                          <reference attribute="ageTimeline"/>
                        </intervalvalue>
                        <reference attribute="minimumAgeLimit"/>
                      </compare>
                      <compare comparison="&lt;">
                        <intervalvalue>
                          <reference attribute="ageTimeline"/>
                        </intervalvalue>
                        <reference attribute="maximumAgeLimit"/>
                      </compare>
                    </members>
                  </fixedlist>
                </all>
              </members>
            </fixedlist>
          </all>
        </timelineoperation>
      </derivation>
    </Attribute>


    <Attribute name="maximumAgeLimit">
      <type>
        <javaclass name="Number"/>
      </type>
      <derivation>
        <specified/>
      </derivation>
    </Attribute>


  </Class>


</RuleSet>