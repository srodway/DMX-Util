/**********************
 Override OneUI Styles 
***********************/

/* The OneUI FilteringSelect widget is styled slightly different to the default
    Select widget. These styles make it look a bit more like the default Select
      widget */
.oneui .dijitComboBox .dijitDownArrowButton {
  border-color: inherit;
  background: #CCC;
  background: -webkit-gradient(linear, left top, left bottom, 
    color-stop(0%,#DBDBDB), color-stop(100%,#F4F4F4));
  background: -moz-linear-gradient(center top , #F4F4F4 0%, #DBDBDB 100%) 
    repeat scroll 0 0 transparent;
  background-color: rgb(244, 244, 244);
}

/* This fixes an issue withe the hover help on the right of the screen */
.oneui .idxOneuiHoverHelpTooltipABRight .idxOneuiHoverHelpTooltipConnector {
  left: auto !important;
  right: 10px;
}

/* Set the font-size for the dropdowns */
.oneui .dijitMenuPopup .dijitSelectMenu .dijitMenuItemLabel {
  font-size: 13.5px;
}

/* Set the font-size for the dropdowns */
.dijitPopup .dijitComboBoxMenuPopup {
  font-size: 13.5px;
}

/* Fix an issue with the hover colour of Select fields */
.oneui .dijitSelectHover{
  border-color: #66D0E9;
}

/* Fix issue with on-focus style for Select fields */
.oneui .dijitSelectFocused {
  border-color: #008abf;
  outline: 0px;
}

/* Update one ui radio button image to include
   selected and disbled state */
.oneui .dijitRadio, .oneui .dijitRadioIcon {
  background-image: url("${resource-uri:radiobutton.icon.external}");
}

/* Pointed to the correct image for
   disable and checked radio buttons */
.oneui .dijitRadioCheckedDisabled {
  background-position: -305px;
}

/* Styling for the Radio Button*/
.oneui div .dijitRadio, .oneui .dijitRadioIcon {
  margin: 4px 2px 0 0;
}

/* Pointed to the correct image for enabled AND mouse hover radio button*/
.oneui .dijitRadioHover {
  /* hovering over an unselected enabled radio button */
  background-position: -321px !important;
}
.oneui .dijitRadioCheckedHover {
  background-position: -241px !important;
}

/* Fix issue with on-focus style for 
   radio buttons  */
.oneui .dijitRadioFocused {
	outline: 1px dotted #666666;
}

