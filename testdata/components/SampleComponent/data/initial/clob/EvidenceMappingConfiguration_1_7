  <map-code-table
    source-codetable="IntakeYesNo"
    target-codetable="AlienStatus"
  >
    <map-value
      source="ITYN4001"
      target="SAS1"
    />
    <map-value
      source="ITYN4002"
      target="SAS2"
    />
  </map-code-table>


  <map-entity source="PractitionerCertificate">
    <target-entity
      id="PractitionerCert"
      name="PractitionerCertificate"
    >
      <map-attribute
        from="comments"
        to="comments"
      />
      <condition expression="PractitionerCertificate.isSignedByClient==&quot;ITYN4001&quot;">
        <set-attribute
          name="claimFormSignedInd"
          value="true"
        />
      </condition>
      <map-attribute
        from="periodLength"
        to="certPeriodCount"
      />
      <map-attribute
        from="certificationPeriodLength"
        to="certPeriodType"
      />
      <map-attribute
        from="medicalCertificateType"
        to="certificateType"
      />
      <map-attribute
        from="certificationStartDate"
        to="certFromDate"
      />
      <map-attribute
        from="practitionerSignedDate"
        to="signatureDate"
      />
      <map-attribute
        from="examinationDate"
        to="examinationDate"
      />
      <map-attribute
        from="certifiedToDate"
        to="specifiedCertToDate"
      />
      <map-attribute
        from="practitionerName"
        to="practitionerName"
      />
      <set-attribute
        name="addressLine1"
        value="Dummy Address Line1"
      />
      <set-attribute
        name="addressLine2"
        value="Dummy Address Line2"
      />
      <set-attribute
        name="city"
        value="NoCity"
      />
      <set-attribute
        name="state"
        value="NoState"
      />
      <set-attribute
        name="zip"
        value="10041"
      />


      <map-entity source="ASCASCDiagnosis">
        <target-entity
          id="ASCDiagnosis"
          name="ASCDiagnosis"
        >
          <map-attribute
            from="diagnosisType"
            to="diagnosis"
          />
          <map-attribute
            from="diagnosisExamDate"
            to="examinationDate"
          />
        </target-entity>
      </map-entity>


    </target-entity>
  </map-entity>


  <!-- Person -->
  <map-entity source="Person">
    <target-entity
      id="householdMember"
      name="ASCHouseholdMember"
    >
      <map-attribute
        from="ssnStatus"
        to="ssnStatus"
      />
      <map-attribute
        from="blackOrAfricanAmerican"
        to="blackOrAfricanAmerican"
      />
      <map-attribute
        from="nativeAlaskanOrAmericanIndian"
        to="nativeAlaskanOrAmericanIndian"
      />
      <map-attribute
        from="asian"
        to="asian"
      />
      <map-attribute
        from="nativeHawaiianOrPacificIslander"
        to="nativeHawaiianOrPacificIslander"
      />
      <map-attribute
        from="whiteOrCaucasian"
        to="whiteOrCaucasian"
      />
      <map-attribute
        from="hispanicOrLatino"
        to="hispanicOrLatino"
      />
      <map-attribute
        from="isMigrantOrSeasonalFarmWorker"
        to="migrantOrSeasonalFarmWorker"
      />
      <map-attribute
        from="householdMemberStartDate"
        to="startDate"
      />


      <condition expression="Person.isUSCitizen==&quot;USC4801&quot;">
        <set-attribute
          name="citizenStatus"
          value="AS4"
        />
      </condition>
      <condition expression="Person.isUSCitizen==&quot;USC4802&quot;">
        <set-attribute
          name="citizenStatus"
          value="AS1"
        />
      </condition>
      <condition expression="Person.isUSCitizen==&quot;USC4803&quot;">
        <set-attribute
          name="citizenStatus"
          value="AS6"
        />
      </condition>
      <condition expression="Person.personResidentInState==&quot;ITYN4001&quot; and Person.residencyStatus==&quot;RS1&quot;">
        <set-attribute
          name="residencyStatus"
          value="RS1"
        />
      </condition>


      <!-- map attribute will not work under condition -->
      <condition expression="Person.personResidentInState==&quot;ITYN4001&quot; and Person.residencyStatus==&quot;RS2&quot;">
        <set-attribute
          name="residencyStatus"
          value="RS2"
        />
      </condition>


      <condition expression="Person.ssn!=0">
        <set-attribute
          name="ssnStatus"
          value="SAS3"
        />
        <set-attribute
          name="veteranStatus"
          value="VS6"
        />
      </condition>


      <!-- map attribute will not work under condition -->
      <condition expression="Person.ssn==0 and Person.ssnStatus==&quot;SAS1&quot;">
        <set-attribute
          name="ssnStatus"
          value="SAS1"
        />
        <set-attribute
          name="veteranStatus"
          value="VS6"
        />
      </condition>


      <condition expression="Person.ssn==0 and Person.ssnStatus==&quot;SAS2&quot;">
        <set-attribute
          name="ssnStatus"
          value="SAS2"
        />
      </condition>


      <condition expression="Person.ssn==0 and Person.ssnStatus==&quot;SAS3&quot;">
        <set-attribute
          name="ssnStatus"
          value="SAS3"
        />
      </condition>


    </target-entity>


    <condition expression="Person.accommodationType!=&quot;&quot;">
      <target-entity
        id="hasLivingArrange"
        name="ASCLivingArrangement"
      >
        <map-attribute
          from="accommodationType"
          to="livingArrangeType"
        />
        <map-attribute
          from="accommodationStatus"
          to="livingArrangeStatus"
        />
        <map-attribute
          from="accommodationStartDate"
          to="startDate"
        />
      </target-entity>
    </condition>


    <condition expression="Person.isUSCitizen==&quot;USC4803&quot;">
      <target-entity
        id="isUSCitizen"
        name="ASCAlien"
      >
        <map-attribute
          from="alienEntryDate"
          to="dateOfEntry"
        />
        <map-attribute
          from="originCountry"
          to="countryOfOrigin"
        />
        <map-attribute
          from="alienRegistrationNumber"
          to="alienRegNumber"
        />
        <map-attribute
          from="alienStatus"
          to="alienStatusOnEntry"
        />
        <map-attribute
          from="alienCategory"
          to="alienType"
        />
      </target-entity>
    </condition>


    <target-entity
      id="IBProductType"
      name="IBProductType"
    >
      <set-attribute
        name="claimType"
        value="IBPT24000"
      />
      <set-attribute
        name="forceIVB"
        value="false"
      />
    </target-entity>


  </map-entity>


</map>
