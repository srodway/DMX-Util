      <derivation>
        <singleitem onEmpty="error" onMultiple="error">
          <readall ruleclass="Group"/>
        </singleitem>
      </derivation>
    </Attribute>
    <Attribute name="relatedPerson">
      <type>
        <ruleclass name="Person"/>
      </type>
      <derivation>
        <singleitem onEmpty="error" onMultiple="error">
          <readall ruleclass="Person">
            <match retrievedattribute="roleID">
              <reference attribute="roleID"/>
            </match>
          </readall>
        </singleitem>
      </derivation>
    </Attribute>
