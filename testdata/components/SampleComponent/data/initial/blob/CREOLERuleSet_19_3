  <Class extends="AbstractFactorList" extendsRuleSet="AssessmentRulesInterface" name="FactorList">
    <Attribute name="factors">
      <type>
        <javaclass name="List">
          <ruleclass name="Factor"/>
        </javaclass>
      </type>
      <derivation>
        <joinlists>
          <fixedlist>
            <listof>
              <javaclass name="List">
                <ruleclass name="Factor"/>
              </javaclass>
            </listof>
            <members>
              <fixedlist>
                <listof>
                  <ruleclass name="Factor"/>
                </listof>
                <members>
                  <reference attribute="finalRiskFactor"/>
                </members>
              </fixedlist>
              <fixedlist>
                <listof>
                  <ruleclass name="Factor"/>
                </listof>
                <members>
                  <reference attribute="determinedSafetyFactor"/>
                </members>
              </fixedlist>
              <reference attribute="visitEvaluationFactor"/>
              <reference attribute="recommendedGoalFactor"/>
              <reference attribute="placementFactor"/>
            </members>
          </fixedlist>
        </joinlists>
      </derivation>
    </Attribute>
    <Attribute name="relatedPerson">
      <type>
        <javaclass name="List">
          <ruleclass name="Person"/>
        </javaclass>
      </type>
      <derivation>
        <readall ruleclass="Person"/>
      </derivation>
    </Attribute>
    <Attribute name="finalRiskFactor">
      <type>
        <ruleclass name="FinalRisk"/>
      </type>
      <derivation>
        <create ruleclass="FinalRisk">
          <specify attribute="factorList">
            <this/>
          </specify>
        </create>
      </derivation>
    </Attribute>
    <Attribute name="visitEvaluationFactor">
      <type>
        <javaclass name="List">
          <ruleclass name="VisitEvaluation"/>
        </javaclass>
      </type>
      <derivation>
        <dynamiclist>
          <list>
            <reference attribute="relatedPerson"/>
          </list>
          <listitemexpression>
            <create ruleclass="VisitEvaluation">
              <specify attribute="roleID">
                <reference attribute="roleID">
                  <current/>
                </reference>
              </specify>
            </create>
          </listitemexpression>
        </dynamiclist>
      </derivation>
    </Attribute>
    <Attribute name="determinedSafetyFactor">
      <type>
        <ruleclass name="DeterminedSafety"/>
      </type>
      <derivation>
        <create ruleclass="DeterminedSafety">
          <specify attribute="factorList">
            <this/>
          </specify>
        </create>
      </derivation>
    </Attribute>
    <Attribute name="recommendedGoalFactor">
      <type>
        <javaclass name="List">
          <ruleclass name="RecommendedGoal"/>
        </javaclass>
      </type>
      <derivation>
        <dynamiclist>
          <list>
            <reference attribute="relatedPerson"/>
          </list>
          <listitemexpression>
            <create ruleclass="RecommendedGoal">
              <specify attribute="roleID">
                <reference attribute="roleID">
                  <current/>
                </reference>
              </specify>
            </create>
          </listitemexpression>
        </dynamiclist>
      </derivation>
    </Attribute>
    <Attribute name="placementFactor">
      <type>
        <javaclass name="List">
          <ruleclass name="Placement"/>
        </javaclass>
      </type>
      <derivation>
        <dynamiclist>
          <list>
            <reference attribute="relatedPerson"/>
          </list>
          <listitemexpression>
            <create ruleclass="Placement">
              <specify attribute="roleID">
                <reference attribute="roleID">
                  <current/>
                </reference>
              </specify>
            </create>
          </listitemexpression>
        </dynamiclist>
      </derivation>
    </Attribute>
  </Class>
  <Class extends="AbstractGroup" extendsRuleSet="AssessmentRulesInterface" name="Group">
    </Class>
  <Class extends="Factor" name="RecommendedGoal">
    <!-- Reference of this factor -->
    <Attribute name="reference">
      <type>
        <javaclass name="String"/>
      </type>
      <derivation>
        <String value="RECOMMENDEDGOAL"/>
      </derivation>
    </Attribute>
    <Attribute name="recommendedGoal">
      <type>
        <ruleclass name="RecommendedGoal"/>
      </type>
      <derivation>
        <singleitem onEmpty="error" onMultiple="error">
          <readall ruleclass="RecommendedGoal"/>
        </singleitem>
      </derivation>
    </Attribute>
    <Attribute name="score">
      <type>
        <javaclass name="String"/>
      </type>
      <derivation>
        <String value=""/>
      </derivation>
    </Attribute>
    <Attribute name="classification">
      <type>
        <codetableentry table="ClassDivisionType"/>
      </type>
      <derivation>
        <reference attribute="recommendedGoal">
          <reference attribute="relatedPerson"/>
        </reference>
      </derivation>
    </Attribute>
  </Class>
  <Class extends="Factor" name="VisitEvaluation">
    <Attribute name="reference">
      <type>
        <javaclass name="String"/>
      </type>
      <derivation>
        <String value="VISIT"/>
      </derivation>
    </Attribute>
    <Attribute name="visitEvaluation">
      <type>
        <ruleclass name="VisitEvaluation"/>
      </type>
      <derivation>
        <singleitem onEmpty="error" onMultiple="error">
          <readall ruleclass="VisitEvaluation"/>
        </singleitem>
      </derivation>
    </Attribute>
    <Attribute name="score">
      <type>
        <javaclass name="String"/>
      </type>
      <derivation>
        <String value=""/>
      </derivation>
    </Attribute>
    <Attribute name="classification">
      <type>
        <codetableentry table="ClassDivisionType"/>
      </type>
      <derivation>
        <choose>
          <type>
            <codetableentry table="ClassDivisionType"/>
          </type>
          <test>
            <reference attribute="finalVisitLevelCode">
              <reference attribute="relatedPerson"/>
            </reference>
          </test>
          <when>
            <condition>
              <Code table="VisitationEvaluation">
                <String value="VE10000"/>
              </Code>
            </condition>
            <value>
              <Code table="ClassDivisionType">
                <String value="CDT10130"/>
              </Code>
            </value>
          </when>
          <when>
            <condition>
              <Code table="VisitationEvaluation">
                <String value="VE10001"/>
              </Code>
            </condition>
            <value>
              <Code table="ClassDivisionType">
                <String value="CDT10131"/>
              </Code>
            </value>
          </when>
          <when>
            <condition>
              <Code table="VisitationEvaluation">
                <String value="VE10002"/>
              </Code>
            </condition>
            <value>
              <Code table="ClassDivisionType">
                <String value="CDT10132"/>
              </Code>
            </value>
          </when>
          <when>
            <condition>
              <Code table="VisitationEvaluation">
                <String value="VE10003"/>
              </Code>
            </condition>
            <value>
              <Code table="ClassDivisionType">
                <String value="CDT10133"/>
              </Code>
            </value>
          </when>
          <when>
            <condition>
              <Code table="VisitationEvaluation">
                <String value="VE20001"/>
              </Code>
            </condition>
            <value>
              <Code table="ClassDivisionType">
                <String value="CDT20001"/>
              </Code>
            </value>
          </when>
          <when>
            <condition>
              <Code table="VisitationEvaluation">
                <String value="VE20002"/>
              </Code>
            </condition>
            <value>
              <Code table="ClassDivisionType">
                <String value="CDT20002"/>
              </Code>
            </value>
          </when>
          <otherwise>
            <value>
              <null/>
            </value>
          </otherwise>
        </choose>
      </derivation>
    </Attribute>
  </Class>
  <Class extends="Factor" name="Placement">
    <Attribute name="reference">
      <type>
        <javaclass name="String"/>
      </type>
      <derivation>
        <String value="PLACEMENT"/>
      </derivation>
    </Attribute>
    <Attribute name="score">
      <type>
        <javaclass name="String"/>
      </type>
      <derivation>
        <String value=""/>
      </derivation>
    </Attribute>
    <Attribute name="classification">
      <type>
        <codetableentry table="ClassDivisionType"/>
      </type>
      <derivation>
        <choose>
          <type>
            <codetableentry table="ClassDivisionType"/>
          </type>
          <when>
            <condition>
              <equals>
                <reference attribute="consecutivePlacementInd">
                  <reference attribute="relatedPerson"/>
                </reference>
                <Code table="RUAIndicator">
                  <String value="RAI10000"/>
                </Code>
              </equals>
            </condition>
            <value>
              <Code table="ClassDivisionType">
                <String value="CDT10141"/>
              </Code>
            </value>
          </when>
          <when>
            <condition>
              <equals>
                <reference attribute="accumulatedPlacementInd">
                  <reference attribute="relatedPerson"/>
                </reference>
                <Code table="RUAIndicator">
                  <String value="RAI10000"/>
                </Code>
              </equals>
            </condition>
            <value>
              <Code table="ClassDivisionType">
                <String value="CDT10142"/>
              </Code>
            </value>
          </when>
          <when>
            <condition>
              <all>
                <fixedlist>
                  <listof>
                    <javaclass name="Boolean"/>
                  </listof>
                  <members>
                    <equals>
                      <reference attribute="consecutivePlacementInd">
                        <reference attribute="relatedPerson"/>
                      </reference>
                      <Code table="RUAIndicator">
                        <String value="RAI10001"/>
                      </Code>
                    </equals>
                    <equals>
                      <reference attribute="accumulatedPlacementInd">
                        <reference attribute="relatedPerson"/>
                      </reference>
                      <Code table="RUAIndicator">
                        <String value="RAI10001"/>
                      </Code>
                    </equals>
                  </members>
                </fixedlist>
              </all>
            </condition>
            <value>
              <Code table="ClassDivisionType">
                <String value="CDT10143"/>
              </Code>
            </value>
          </when>
          <otherwise>
            <value>
              <Code table="ClassDivisionType">
                <String value=""/>
              </Code>
            </value>
          </otherwise>
        </choose>
      </derivation>
    </Attribute>
  </Class>
