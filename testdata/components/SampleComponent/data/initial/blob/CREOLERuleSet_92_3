              </reference>
            </value>
          </when>
          <otherwise internalID="id1355753083320">
            <value internalID="id1355753083321">
              <null internalID="id1355753083322"/>
            </value>
          </otherwise>
        </choose>
      </derivation>
    </Attribute>
