  <Class extends="AbstractFactorList" extendsRuleSet="AssessmentRulesInterface" name="FactorList">
    <Attribute name="factors">
      <type>
        <javaclass name="List">
          <ruleclass name="Factor"/>
        </javaclass>
      </type>
      <derivation>
        <fixedlist>
          <listof>
            <ruleclass name="Factor"/>
          </listof>
          <members>
            <reference attribute="recommendedGoalFactor"/>
          </members>
        </fixedlist>
      </derivation>
    </Attribute>
    <Attribute name="recommendedGoalFactor">
      <type>
        <ruleclass name="RecommendedGoal"/>
      </type>
      <derivation>
        <create ruleclass="RecommendedGoal">
          <specify attribute="roleID">
            <reference attribute="roleID"/>
          </specify>
          <specify attribute="factorList">
            <this/>
          </specify>
        </create>
      </derivation>
    </Attribute>
  </Class>


  <Class extends="Factor" name="RecommendedGoal">


    <!-- Reference of this factor -->
    <Attribute name="reference">
      <type>
        <javaclass name="String"/>
      </type>
      <derivation>
        <String value="RECOMMENDEDGOAL"/>
      </derivation>
    </Attribute>


    <Attribute name="recommendedGoal">
      <type>
        <ruleclass name="RecommendedGoal"/>
      </type>
      <derivation>
        <singleitem onEmpty="error" onMultiple="error">
          <readall ruleclass="RecommendedGoal"/>
        </singleitem>
      </derivation>
    </Attribute>


    <Attribute name="score">
      <type>
        <javaclass name="String"/>
      </type>
      <derivation>
        <String value=""/>
      </derivation>
    </Attribute>


    <Attribute name="classification">
      <type>
        <codetableentry table="ClassDivisionType"/>
      </type>
      <derivation>
        <choose>
          <type>
            <codetableentry table="ClassDivisionType"/>
          </type>
          <when>
            <condition>
              <reference attribute="reunifyInd"/>
            </condition>
            <value>
              <Code table="ClassDivisionType">
                <String value="CDT10136"/>
              </Code>
            </value>
          </when>
          <when>
            <condition>
              <reference attribute="changePermanencyGoalInd"/>
            </condition>
            <value>
              <Code table="ClassDivisionType">
                <String value="CDT10135"/>
              </Code>
            </value>
          </when>
          <when>
            <condition>
              <reference attribute="outOfHomeCareInd"/>
            </condition>
            <value>
              <Code table="ClassDivisionType">
                <String value="CDT10134"/>
              </Code>
            </value>
          </when>
          <when>
            <condition>
              <reference attribute="changeGoalPlacementInd"/>
            </condition>
            <value>
              <Code table="ClassDivisionType">
                <String value="CDT10135"/>
              </Code>
            </value>
          </when>
          <otherwise>
            <value>
              <Code table="ClassDivisionType">
                <String value="CDT10134"/>
              </Code>
            </value>
          </otherwise>
        </choose>
      </derivation>
    </Attribute>


    <Attribute name="reunifyInd">
      <type>
        <javaclass name="Boolean"/>
      </type>
      <derivation>
        <all>
          <fixedlist>
            <listof>
              <javaclass name="Boolean"/>
            </listof>
            <members>
              <reference attribute="riskLevelGroupOneInd"/>
              <reference attribute="safetyLevelGroupOneInd"/>
              <reference attribute="evaluationGroupOneInd"/>
            </members>
          </fixedlist>
        </all>
      </derivation>
    </Attribute>


    <Attribute name="outOfHomeCareInd">
