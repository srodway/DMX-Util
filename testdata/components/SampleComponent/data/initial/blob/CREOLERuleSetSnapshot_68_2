    <Attribute name="allServicesGroups">
      <type>
        <javaclass name="List">
          <ruleclass name="Service"/>
        </javaclass>
      </type>
      <derivation>
        <fixedlist>
          <listof>
            <ruleclass name="Service"/>
          </listof>
          <members>
            <create ruleclass="EducationalPrograms">
              <reference attribute="education"/>
            </create>
            <create ruleclass="ChildCareServices">
              <reference attribute="education"/>
            </create>
            <create ruleclass="FinancialServices">
              <reference attribute="finance"/>
            </create>
            <create ruleclass="Shelter">
              <reference attribute="foodShelter"/>
            </create>
            <create ruleclass="FoodServices">
              <reference attribute="foodShelter"/>
            </create>
            <create ruleclass="SubstanceAbusePrograms">
              <reference attribute="medicalHealthSubsAbuse"/>
            </create>
            <create ruleclass="MentalHealthServices">
              <reference attribute="medicalHealthSubsAbuse"/>
            </create>
            <create ruleclass="HealthCareServices">
              <reference attribute="physicalHealthDisability"/>
            </create>
            <create ruleclass="DisabilityRelatedServices">
              <reference attribute="physicalHealthDisability"/>
            </create>
            <create ruleclass="SafetyRelatedServices">
              <reference attribute="safety"/>
            </create>
          </members>
        </fixedlist>
      </derivation>
    </Attribute>


    <Attribute name="services">
      <type>
        <javaclass name="List">
          <ruleclass name="Service"/>
        </javaclass>
      </type>
      <derivation>
        <filter>
          <list>
            <reference attribute="allServicesGroups"/>
          </list>
          <listitemexpression>
            <equals>
              <reference attribute="appropriate">
                <current/>
              </reference>
              <true/>
            </equals>
          </listitemexpression>
        </filter>
      </derivation>
    </Attribute>


    <Attribute name="allNeeds">
