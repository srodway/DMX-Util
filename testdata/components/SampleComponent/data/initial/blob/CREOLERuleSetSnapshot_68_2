  <Class name="Triage">
    <Attribute name="description">
      <type>
        <javaclass name="curam.creole.value.Message"/>
      </type>
      <derivation>
        <ResourceMessage key="Triage.description" resourceBundle="curam.triage.rules.TriageRuleSetProperties"/>
      </derivation>
    </Attribute>


    <!-- 6 attributes to determine the type of help that the citizen requires -->
    <Attribute name="finance">
      <type>
        <ruleclass name="Finance"/>
      </type>
      <derivation>
        <!--<create ruleclass="Finance"></create>-->
        <singleitem onEmpty="error" onMultiple="error">
          <readall ruleclass="Finance"/>
        </singleitem>
      </derivation>
    </Attribute>


    <Attribute name="foodShelter">
      <type>
        <ruleclass name="FoodShelter"/>
      </type>
      <derivation>
        <!--<create ruleclass="FoodShelter"></create>-->
        <singleitem onEmpty="error" onMultiple="error">
          <readall ruleclass="FoodShelter"/>
        </singleitem>
      </derivation>
    </Attribute>


    <Attribute name="physicalHealthDisability">
