      </derivation>
    </Attribute>


    <Attribute name="isClientHasExistingTriages">
      <type>
        <javaclass name="Boolean"/>
      </type>
      <derivation>
        <compare comparison="&gt;">
          <property name="size">
            <object>
              <!-- BEGIN, CR00260917, CP-->
              <reference attribute="filteredTriages"/>
              <!-- END, CR00260917-->
            </object>
          </property>
          <Number value="0"/>
        </compare>
      </derivation>
    </Attribute>


    <!-- BEGIN, CR00260917, CP-->
    <!-- List of Filetered Triage which are not in Cancelled state. -->
    <Attribute name="filteredTriages">
      <type>
        <javaclass name="List">
          <ruleclass name="ParticipantTriage"/>
        </javaclass>
      </type>
      <derivation>
        <filter>
          <list>
            <reference attribute="participantTriages"/>
          </list>
          <listitemexpression>
            <equals>
              <reference attribute="recordStatus">
                <current/>
              </reference>
              <Code table="RecordStatus">
                <String value="RST1"/>
              </Code>
            </equals>


          </listitemexpression>
        </filter>


      </derivation>
    </Attribute>
    <!-- END, CR00260917-->


    <Attribute name="participantTriages">
      <type>
        <javaclass name="List">
          <ruleclass name="ParticipantTriage"/>
        </javaclass>
      </type>
      <derivation>
        <readall ruleclass="ParticipantTriage">
          <match retrievedattribute="concernRoleID">
            <reference attribute="concernRoleID"/>


          </match>
        </readall>
      </derivation>
    </Attribute>


