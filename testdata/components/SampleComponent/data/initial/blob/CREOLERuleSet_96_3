        <choose>
          <type>
            <javaclass name="Boolean"/>
          </type>
          <when>
            <condition>
              <not>
                <equals>
                  <reference attribute="toDate">
                    <reference attribute="evidence"/>
                  </reference>
                  <null/>
                </equals>
              </not>
            </condition>
            <value>
              <true/>
            </value>
          </when>
          <otherwise>
            <value>
              <false/>
            </value>
          </otherwise>
        </choose>


      </derivation>


    </Attribute>
